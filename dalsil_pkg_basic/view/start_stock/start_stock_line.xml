<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <!--#########################################################################################################-->
        <!--Group Tree View-->
        <!--#########################################################################################################-->
        <record model="ir.ui.view" id="view_dalsil_start_stock_line_tree">
            <field name="name">dalsil.start_stock.line.tree</field>
            <field name="model">dalsil.start_stock.line</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="parent_id" invisible="1"/>
                    <field name="product_id"/>
                    <field name="uom_id"/>
                    <field name="stock"/>
                    <field name="location_id"/>
                    <field name="inventory_value"/>
                </tree>
            </field>
        </record>

        <!--#########################################################################################################-->
    </data>
</odoo>
