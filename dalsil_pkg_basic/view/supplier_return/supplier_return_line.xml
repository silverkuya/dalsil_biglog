<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <!--#########################################################################################################-->
        <!--Group Tree View-->
        <!--#########################################################################################################-->
        <record model="ir.ui.view" id="view_dalsil_supplier_return_line_tree">
            <field name="name">dalsil.supplier_return.line.tree</field>
            <field name="model">dalsil.supplier_return.line</field>
            <field name="arch" type="xml">
                <tree editable="bottom" create="0">
                    <field name="parent_id" invisible="1"/>
                    <field name="product_id" readonly="1"/>
                    <field name="account_id" readonly="1"/>
                    <field name="qty" readonly="1"/>
                    <field name="qty_return"/>
                    <field name="unit_price" readonly="1"/>
                    <field name="invoice_line_tax_ids" readonly="1"/>
                    <field name="sub_total" readonly="1"/>
                </tree>
            </field>
        </record>

        <!--#########################################################################################################-->
    </data>
</odoo>
